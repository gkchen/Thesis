doi = {10.1021/acs.jpca.5b03740},
file = {:C\:/Users/scientist/Documents/Mendeley Desktop/Song, Guo/Journal of Physical Chemistry A/Song, Guo - 2015 - Vibrational and Rotational Mode Specificity in the Cl H$_2$O $rightarrow$ HCl OH Reaction A Quantum Dynamical Study.pdf:pdf},
