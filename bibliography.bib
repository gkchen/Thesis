number = {11},
pages = {7821--7833},
publisher = {Royal Society of Chemistry},
title = {{Effects of vibrational excitation on the F + H2O $\rightarrow$ HF + OH reaction: Dissociative photodetachment of overtone-excited [F-H-OH]$^-$}},
