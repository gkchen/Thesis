@article{Adams1976,
abstract = {A technique is described by which a mass-selected, low-energy, positive ion beam, derived from a gas-discharge source in conjunction with a quadrupole mass filter, is injected into a flowing gas. The subsequent reaction of these ions with neutral molecules introduced into the carrier gas downstream of the injection point is studied using downstream mass spectrometer sampling in a manner identical to the well established flowing afterglow technique. We have termed our apparatus SIFT (Selected Ion Flow Tube). The rate coefficients for some sixteen ion-molecule reactions at room temperature measured using SIFT are presented, many of which have previously been reported. The branching ratio of the (He+ + N2)-reaction is also reported and the value compared with those previously obtained. Some particular points of merit of the SIFT technique are discussed and ways in which it may be utilized to investigate some outstanding problems in ionic reaction studies are indicated. {\textcopyright} 1976.},
author = {Adams, N. G. and Smith, D.},
doi = {10.1016/0020-7381(76)80133-7},
file = {:C\:/Users/scientist/Documents/Mendeley Desktop/Adams, Smith/International Journal of Mass Spectrometry and Ion Physics/Adams, Smith - 1976 - The selected ion flow tube (SIFT) A technique for studying ion-neutral reactions.pdf:pdf},
issn = {00207381},
journal = {International Journal of Mass Spectrometry and Ion Physics},
number = {3-4},
pages = {349--359},
title = {{The selected ion flow tube (SIFT); A technique for studying ion-neutral reactions}},
volume = {21},
year = {1976}
}
