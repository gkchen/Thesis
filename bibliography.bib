file = {:C\:/Users/scientist/Documents/Mendeley Desktop/Yang et al/J. Phys. Chem. Lett/Yang et al. - 2018 - Optical Control of Reactions between Water and Laser-Cooled Be$$ Ions.pdf:pdf},
issn = {19487185},
journal = {J. Phys. Chem. Lett},
pages = {28},
publisher = {UTC},
title = {{Optical Control of Reactions between Water and Laser-Cooled Be$^+$ Ions}},
url = {https://pubs.acs.org/sharingguidelines},
volume = {9},
year = {2018}
}
