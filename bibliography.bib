eprint = {1708.07832},
file = {:C\:/Users/scientist/Documents/Mendeley Desktop/Tomza et al/Unknown/Tomza et al. - 2017 - Cold hybrid ion-atom systems.pdf:pdf},
pages = {1--61},
title = {{Cold hybrid ion-atom systems}},
url = {http://arxiv.org/abs/1708.07832},
year = {2017}
}
