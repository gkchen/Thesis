journal = {Journal of Physical Chemistry A},
number = {24},
pages = {6188--6194},
title = {{Vibrational and Rotational Mode Specificity in the Cl + H$_2$O $\rightarrow$ HCl + OH Reaction: A Quantum Dynamical Study}},
volume = {119},
year = {2015}
}
@article{Ray2017,
abstract = {Photodetaching vibrationally excited FH 2 O − channels energy into the reaction coordinate of the F + H 2 O reaction, as shown in this joint experimental-theoretical study. The reaction F + H 2 O → HF + OH is a four-atom system that provides an important benchmark for reaction dynamics. Hydrogen atom transfer at the transition state for this reaction is expected to exhibit a strong dependence on reactant vibrational excitation. In the present study, the vibrational effects are examined by photodetachment of vibrationally excited F − (H 2 O) precursor anions using photoelectron-photofragment coincidence (PPC) spectroscopy and compared with full six-dimensional quantum dynamical calculations on ab initio potential energy surfaces. Prior to photodetachment at h$\nu$ UV = 4.80 eV, the overtone of the ionic hydrogen bond mode in the precursor F − (H 2 O), 2 $\nu$ IHB at 2885 cm −1 , was excited using a tunable IR laser. Experiment and theory show that vibrational energy in the anion can be effectively carried away by the photoelectron upon a Franck–Condon photodetachment, and also show evidence for an increase of branching into the F + H 2 O reactant channel. The experimental results suggest a greater role for product rotational excitation than theory. Improved potential energy surfaces and longer wavepacket propagation times would be helpful to further examine the nature of the discrepancy.},
